@model AlbumCollection.Models.Album
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Details of the Album</title>
</head>
<body>
    @foreach (var song in Model.Songs)
    {

        
    <h1>
        @song.SongName
    </h1>
    <h2>
        @song.SongId
    </h2>
    }
</body>
</html>
